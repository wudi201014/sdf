{
   int stock;
   int commande;

   pthread_t thread_store;
   pthread_t thread_clients [5];

   pthread_mutex_t *mutex_stock;
   pthread_mutex_t *mutex_commande;

   void (*print_stock)(int);
   void (*print_client_action)(int,int);
} store_t;

