   pthread_mutex_init((*store)->mutex_commande, ((void *)0));

}

void free_store(store_t *store){
   free(store->mutex_stock);
   free(store->mutex_commande);
   free(store);
}


int simulate_store(store_t *store){
   int i = 0;
   int ret = 0;




         ret = pthread_create (
            & store->thread_store, ((void *)0),
            fn_store, (void *) store
            );


         if (! ret)
         {

            for (i = 0; i < 5; i++)
            {
               ret = pthread_create (
                  & store->thread_clients [i], ((void *)0),
                  fn_clients, (void *) store);

            }
         }


         i = 0;
         for (i = 0; i < 5; i++)
         {
            pthread_join (store->thread_clients [i], ((void *)0));
         }
         pthread_join (store->thread_store, ((void *)0));


         return 0;
      }
